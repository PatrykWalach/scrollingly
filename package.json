{
  "name": "scrollingly",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "npm run generate && vue-cli-service build --modern",
    "lint": "vue-cli-service lint",
    "generate": "graphql-codegen --config codegen.yml",
    "generate:watch": "graphql-codegen --config codegen.yml --watch"
  },
  "dependencies": {
    "@apollo/client": "^3.3.20",
    "@mdi/font": "5.9.55",
    "@vue/apollo-composable": "^4.0.0-alpha.12",
    "apollo-link-rest": "^0.8.0-beta.0",
    "camelcase": "^6.2.0",
    "core-js": "^3.6.5",
    "fscreen": "^1.2.0",
    "graphql": "^15.5.1",
    "graphql-anywhere": "^4.2.7",
    "register-service-worker": "^1.7.1",
    "roboto-fontface": "*",
    "snake-case": "^3.0.4",
    "swiper": "6.7.1",
    "vue": "3.1",
    "vue-router": "4",
    "vuetify": "^3.0.0-alpha.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.21.5",
    "@graphql-codegen/fragment-matcher": "2.0.1",
    "@graphql-codegen/typescript": "1.22.3",
    "@graphql-codegen/typescript-apollo-client-helpers": "^1.1.8",
    "@graphql-codegen/typescript-operations": "1.18.2",
    "@graphql-codegen/typescript-vue-apollo": "^2.3.7",
    "@types/fscreen": "^1.0.1",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-pwa": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-typescript": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.1.2",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "7.11.1",
    "lint-staged": "^9.5.0",
    "prettier": "^2.2.1",
    "react": "^17.0.2",
    "sass": "~1.32.0",
    "sass-loader": "^10.0.0",
    "typescript": "~4.1.5",
    "vue-cli-plugin-vuetify": "~2.4.1"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,vue,ts,tsx}": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
